{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Flask (flask run)",
      "type": "python",
      "request": "launch",
      "module": "flask",
      "python": "${workspaceFolder}\\venv\\Scripts\\python.exe",
      "env": {
        "FLASK_APP": "run.py",
        "FLASK_ENV": "development",
        "FLASK_DEBUG": "1"
      },
      "args": ["run", "--no-debugger", "--no-reload"],
      "jinja": true,
      "console": "integratedTerminal"
    }
  ]
}